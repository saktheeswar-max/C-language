#include<stdio.h>
int main(){
    int n,a,b;
    printf("enter the  multiplicand number:");
    scanf("%d",&n);
    printf("enter the how many multiplier:");
    scanf("%d",&a);
    for(int i=1;i <= a;i++){
        if(i!=1& i!=16){
        b=i*n;
        printf("%d X %d = %d\n",i,n,b);
        }
    }
    return 0;
}
